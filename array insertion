public class Main
{
	public static void main(String[] args) {
		int a[]=new int[10];
		a[0]=10;a[1]=4;a[2]=6;a[3]=16;a[4]=24;
		int n=6;
		for (int i=0;i<n;i++)
		{
		    System.out.print(a[i] +"," );
		   
		}
		 System.out.println();
		int x=20;
		int index=2;
		for(int i=n;i>index;i--)
		{
		    a[i]=a[i-1];
		    a[index]=x;
		}
			for (int i=0;i<n+1;i++)
		{
		    System.out.print(a[i] +"," );
		}
		
	
	}
}
